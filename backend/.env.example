# PartLogic Backend Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
DEBUG=false
API_TITLE=PartLogic API
API_VERSION=1.0.0

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# eBay API Configuration
# Get your keys from: https://developer.ebay.com/
EBAY_APP_ID=your_ebay_app_id_here
EBAY_CERT_ID=your_ebay_cert_id_here
EBAY_DEV_ID=your_ebay_dev_id_here
EBAY_SANDBOX=true

# Scraping Configuration
REQUEST_TIMEOUT=30
RATE_LIMIT_DELAY=1.0
MAX_RESULTS_PER_SOURCE=20

# Car-Part.com Configuration
CARPART_DEFAULT_ZIP=
